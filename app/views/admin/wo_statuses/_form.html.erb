<%= simple_form_for [:admin, @wo_status] do |f| %>
  <%= f.error_notification %>
  <div class="form-inputs">
	<%= f.input :status %>
	<%= f.input :style %>
  </div>
  <div class="form-actions">
    <%= f.button :submit, :class=>"btn btn-primary" %>
		<%= link_to 'Back', admin_wo_statuses_path, :class=>"btn" %>
  </div>
<% end %>
